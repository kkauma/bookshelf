{
  "version": 2,
  "builds": [
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "^/data/.*\\.js$",
      "headers": {
        "Content-Type": "application/javascript"
      },
      "dest": "/$0"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
